{
    "version": "1.0",
    "configurations": [
        {
            "name": "Default debugging: project",
            "type": "python",
            "request": "launch",
            "console": "none",
            "internalConsoleOptions": "openOnSessionStart",
            "program": "${workspaceFolder}/app/__main__.py",
            "args": [],
        },
        {
            "name": "Default indra: project",
            "type": "python",
            "request": "launch",
            "console": "integratedTerminal",
            "program": "${workspaceFolder}",
            "args": [],
            "pythonPath": "D:\\anaconda3\\envs\\indra\\python.exe"
        },
        {
            "name": "Python: Current File (Integrated Terminal)",
            "type": "python",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal",
            "pythonPath": "${config:python.pythonPath}"
        }
    ]
}